{
  "name": "poe-trademaster",
  "version": "1.7.0",
  "description": "Trademaster tool for path of exile",
  "author": {
    "name": "Stefan Kunde",
    "email": "stefankunde89@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 11",
    "electron",
    "path of exile",
    "poe",
    "trademaster",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StefanKunde/Trademaster-Electron-Path-of-exile-tool.git"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run rebuildRobotJs && npm run build:prod && electron-builder build --publish=never --config.productName=Poe-Trademaster",
    "electron:publish": "npm run rebuildRobotJs && npm run build:prod && electron-builder build --publish=always",
    "rebuildRobotJs": "npm rebuild --runtime=electron --target=11.3.0 --disturl=https://atom.io/download/atom-shell --abi=85",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags"
  },
  "dependencies": {
    "@electron/remote": "1.0.4",
    "axios": "0.19.2",
    "electron-overlay-window": "2.0.1",
    "electron-settings": "4.0.2",
    "electron-updater": "4.3.9",
    "ngx-mat-select-search": "3.2.0",
    "node-cache": "5.1.2",
    "robotjs": "0.6.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-devkit/build-angular": "15.0.4",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/animations": "11.2.8",
    "@angular/cdk": "11.2.12",
    "@angular/cli": "11.2.7",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/compiler-cli": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.8",
    "@angular/language-service": "11.2.8",
    "@angular/material": "11.2.12",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "@electron-forge/publisher-github": "*",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.6.7",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.15.0",
    "@typescript-eslint/parser": "4.16.1",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.6.5",
    "cross-env": "7.0.3",
    "electron": "12.0.2",
    "electron-builder": "22.10.5",
    "electron-reload": "1.5.0",
    "eslint": "7.22.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
